name = Typo
description = A drupal theme of snappytheme.com.
core = 7.x
base theme = bootstrap


;;;;;;;;;;;;;;;;;;;;;
;; Regions
;;;;;;;;;;;;;;;;;;;;;
regions[home_section]   = 'Home Section'
regions[navigation]     = 'Navigation'
regions[header]         = 'Top Bar'
regions[highlighted]    = 'Highlighted'
regions[help]           = 'Help'
regions[content]        = 'Content'
regions[sidebar_first]  = 'Primary'
regions[sidebar_second] = 'Secondary'
regions[footer]         = 'Footer'
regions[page_top]       = 'Page top'
regions[page_bottom]    = 'Page bottom'


;;;;;;;;;;;;;;;;;;;;;
;; Stylesheets
;;;;;;;;;;;;;;;;;;;;;

stylesheets[all][] = css/bootstrap.css
stylesheets[all][] = css/overrides.css
stylesheets[all][] = css/animate.css
stylesheets[all][] = css/font-awesome.css
stylesheets[all][] = css/jquery.bxslider.css
stylesheets[all][] = css/flexslider.css
stylesheets[all][] = css/magnific-popup.css
stylesheets[all][] = css/style.css
stylesheets[all][] = css/responsive.css

; For information on choosing the desired method, please read the Drupal
