scripts[] = 'js/jquery.migrate.js'
scripts[] = 'js/bootstrap.js'
scripts[] = 'js/jquery.imagesloaded.min.js'
scripts[] = 'js/waypoint.min.js'
;scripts[] = 'js/smooth-scroll.js'
;scripts[] = 'js/plugins-scroll.js'
scripts[] = 'js/jquery.magnific-popup.min.js'	
scripts[] = 'js/script.js'	

